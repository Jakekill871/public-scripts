DFCheck()
end)

Tab5:AddSwitch("Auto Punch/Click(For Auto Farm)", function(autoclicktoggle)
        if autoclicktoggle == true then
        wait(1)
        _G.AutoPunch = true
        else
        _G.AutoPunch = false
        end
end)

